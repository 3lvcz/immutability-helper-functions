{
  "name": "immutability-helper-functions",
  "version": "2.8.1",
  "description": "",
  "main": "lib/ihf.js",
  "unpkg": "dist/ihf.js",
  "module": "es/ihf.js",
  "typings": "./index.d.ts",
  "files": [
    "lib",
    "es",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf lib dist es",
    "format": "prettier --write \"{src,test}/**/*.ts\" index.d.ts \"./*.{md,json}\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.ts\" index.d.ts \"./*.{md,json}\"",
    "eslint": "eslint index.js",
    "tslint": "tslint index.d.ts",
    "lint": "npm run --silent eslint && npm run --silent tslint",
    "lint:staged": "lint-staged",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register test/*_*.js",
    "test:inspect": "cross-env NODE_ENV=test mocha --inspect-brk --require @babel/register test/*_*.js",
    "build": "rollup -c",
    "prepare": "npm run clean && npm run format:check && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3lvcz/immutability-helper-functions.git"
  },
  "keywords": [
    "immutability"
  ],
  "author": "Evgeniy Beklov <beklowea@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3lvcz/immutability-helper-functions/issues"
  },
  "homepage": "https://github.com/3lvcz/immutability-helper-functions#readme",
  "dependencies": {
    "immutability-helper": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^3.0.0"
  },
  "optionalDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^8.2.3",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "expect": "^23.6.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "mocha": "^5.2.0",
    "prettier": "^1.14.3",
    "sinon": "^7.1.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.3"
  },
  "peerDependencies": {
    "immutability-helper": "^2.8.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add",
      "eslint"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  }
}
